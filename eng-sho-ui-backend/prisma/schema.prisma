generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model words {
  id      Int    @id @default(autoincrement()) @db.UnsignedInt
  english String @db.VarChar(512)
  shona   String @db.VarChar(512)
}
